<?xml version="1.0" encoding="utf-8" standalone="no"?><slim xmlns="www.sap.com/slim" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" slimEd="1.0" xsi:schemaLocation="www.sap.com/slim slim.xsd">
    <header>
        <locale>
            <develLc>EN</develLc>
            <contentLc>zh_CN</contentLc>
        </locale>
        <objType>line</objType>
        <collection>ANYWHERE_OCC</collection>
        <domain>AW</domain>
        <contentName>OCC_APP_REPORT_DIRECT_SALES</contentName>
        <description>A full featured SLIM-file</description>
        <defaults>
            <maxL>255</maxL>
        </defaults>
        <workflow>
            <contacts>
                <developer>I058953</developer>
            </contacts>
        </workflow>
    </header>
    <body>
        <loGrp grpClass="textType" grpName="XMSG">
            <lo maxL="200" tK="app.report.directsales.message">
                <loC>(just a sample) xx error message</loC>
            </lo>
        </loGrp>
        <loGrp grpClass="textType" grpName="XFLD">
            <lo maxL="200" tK="label.winlostReason.title">
                <loC>赢单丢单原因</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.lost">
                <loC>丢单数量</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.win">
                <loC>赢单数量</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.winlostnumber">
                <loC>数量</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.winlostamount">
                <loC>金额</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.customer">
                <loC>客户</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.stage">
                <loC>阶段</loC>
            </lo>
            <lo maxL="200" tK="label.winlostReason.lostpotentialamount">
