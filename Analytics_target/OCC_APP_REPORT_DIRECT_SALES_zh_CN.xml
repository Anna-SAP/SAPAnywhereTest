            <lo maxL="200" tK="label.winlostReason.reason">
                <loC>理由</loC>
            </lo>
            <lo maxL="200" tK="label.leadSourceSuccess.title">
                <loC>成功的销售线索</loC>
            </lo>
            <lo maxL="200" tK="label.leadSourceSuccess.description">
                <loC>有多少销售线索能成功生成销售机会和报价？这个报表会按照来源进行分类，为你自动归纳这些有价值的线索。</loC>
            </lo>
            <lo maxL="200" tK="label.leadSourceSuccess.source">
                <loC>来源</loC>
            </lo>
            <lo maxL="200" tK="label.leadSourceSuccess.count">
                <loC>数量</loC>
            </lo>
            <lo maxL="200" tK="label.leadSourceSuccess.countColumn">
                <loC>销售线索数量</loC>
            </lo>
            <lo maxL="200" tK="label.directSales.startDate">
                <loC>起始日期</loC>
            </lo>
            <lo maxL="200" tK="label.directSales.endDate">
                <loC>结束日期</loC>
            </lo>
            <lo maxL="200" tK="label.customer360.name">
                <loC>客户360</loC>
            </lo>
            <lo maxL="200" tK="label.customer360.param.name">
                <loC>客户</loC>
            </lo>
            <lo maxL="200" tK="label.customer360.param.hint">
                <loC>客户名称</loC>
            </lo>
            <lo maxL="200" tK="label.customerInsight.name">
                <loC>业务洞察</loC>
            </lo>
            <lo maxL="200" tK="label.customerInsight.param.hint">
                <loC>客户名称</loC>
            </lo>
